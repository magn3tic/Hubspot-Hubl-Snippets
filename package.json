{
    "name": "hubspot-hubl-snippets",
    "displayName": "Hubspot-Hubl-Snippets",
    "description": "A Hubspot Hubl Snippet Library",
    "version": "0.0.1",
    "publisher": "maglabs",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "images/magnetic-logo.jpg",
    "categories": [
        "Snippets"
    ],
    "hompage": "http://magneticcreative.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/magn3tic/Hubspot-Hubl-Snippets.git"
    },
    "keywords": [
        "Magnetic Creative",
        "Hubspot",
        "Hubl"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "snippets": [{
            "language": "html",
            "path": "./snippets/html.json"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}